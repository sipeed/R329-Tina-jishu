		/* kld35512-_FT6336G and nv3029s-_FT6236U */
		twi1: twi@0x02502400{
			status = "okay";
			touchscreen: fts@38 {
				compatible = "focaltech,fts";
				reg = <0x38>;
				focaltech,reset-gpio = <&pio PH 7 1 1 1 1>;
				focaltech,irq-gpio = <&r_pio PM 5 6 1 1 1>;
				focaltech,max-touch-number = <1>;
				focaltech,display-coords = <0 0 240 320>;

				/*focaltech,have-key;*/
				focaltech,key-number = <0>;
				focaltech,keys = <139 102 158>;
				focaltech,key-y-coord = <2000>;
				focaltech,key-x-coords = <200 600 800>;
			};
		};

		keyboard0:keyboard{
			compatible = "allwinner,keyboard_1350mv";
			reg = <0x0 0x07030800 0x0 0x400>;
			clocks = <&clk_lradc>;
			interrupts = <GIC_SPI 115 IRQ_TYPE_NONE>;
			status = "okay";
			key_cnt = <5>;

			key0 = <160 115>;
			key1 = <317 114>;
			key2 = <474 248>;
			key3 = <614 164>;
			key4 = <880 102>;

			// key0 = <210 115>;
			// key1 = <410 114>;
			// key2 = <590 248>;
			// key3 = <750 164>;
			// key4 = <880 102>;
			wakeup-source;
		};

