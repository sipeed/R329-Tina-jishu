		// };

		lcd_fb0: lcd_fb0@0 {
			lcd_used = <1>;
			lcd_driver_name = "nv3029s";
			lcd_pwm_freq = <5000>;
			lcd_pwm_ch = <4>;
			lcd_pwm_used = <1>;
			lcd_y = <240>;
			lcd_x = <240>;
			lcd_spi_bus_num = <1>;
			lcd_data_speed = <60>;
			lcd_if = <0>;
			lcd_pwm_pol = <0>;
			lcd_pixel_fmt = <10>;
			lcd_rgb_order = <0>;
			// fb_buffer_num = <1>; // default 2
			lcd_backlight = <200>;
			lcd_fps = <60>;
			lcd_spi_dc_pin = <&pio PH 4 1 1 2 1>;
			// lcd_gpio_0 = <&pio PH 4 1 1 2 1>;
			// lcd_bl_en = <&pio PH 2 1 1 2 1>;
			lcd_dbi_te = <0>;

		};

